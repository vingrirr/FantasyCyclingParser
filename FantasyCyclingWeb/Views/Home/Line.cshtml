@using FantasyCyclingParser
@using FantasyCyclingWeb.Models

 @model SnapshotViewModel
    

    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div id="lineChart" style="height:600px;">
            <svg></svg>
        </div>

    </div>

    <script type="text/javascript">
