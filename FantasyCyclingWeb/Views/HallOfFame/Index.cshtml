@{
    ViewBag.Title = "Hall of Fame";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}



@using FantasyCyclingParser
@using FantasyCyclingWeb.Models

@model  HallOfFameViewModel


<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="panel panel-flat">
        <div class="panel-heading">

            <h5>Hall of Fame - 150 points</h5>
            <hr />
        </div>
        <div class="panel-body">
            <!-- Year, PDCTeam Name, Total Points as an accordian....then accordian expand to show the riders and points scored -->
            @foreach (HallOfFameModel vm in Model.Entries)
            {
                @Html.Partial("_HoFentry", vm)
            }


        </div>
    </div>
</div>



